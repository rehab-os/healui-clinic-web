{
  "evidence_based_protocols": {
    "PROTOCOL_001": {
      "name": "Adhesive Capsulitis Evidence-Based Protocol",
      "condition": "COND_001",
      "evidence_level": "Grade_A",
      "phases": {
        "phase_1_freezing": {
          "duration": "0-9_months",
          "characteristics": ["progressive_pain", "night_pain", "ROM_loss"],
          "goals": ["pain_management", "maintain_available_ROM", "patient_education"],
          "interventions": {
            "exercises": ["EX_003", "EX_048", "gentle_ROM"],
            "modalities": ["EQ_007", "EQ_009", "heat"],
            "manual_therapy": ["grade_1-2_mobilization"],
            "medications": ["NSAIDs", "corticosteroid_injection"]
          },
          "precautions": ["avoid_aggressive_stretching", "respect_pain_levels"],
          "progression_criteria": ["decreased_night_pain", "improved_pain_with_movement"]
        },
        "phase_2_frozen": {
          "duration": "4-12_months", 
          "characteristics": ["stable_ROM_loss", "decreased_pain"],
          "goals": ["restore_ROM", "maintain_strength", "improve_function"],
          "interventions": {
            "exercises": ["EX_001", "EX_021", "EX_037_for_shoulder", "EX_002"],
            "modalities": ["heat", "EQ_008_ultrasound"],
            "manual_therapy": ["grade_3-4_mobilization", "stretching"],
            "advanced": ["manipulation_under_anesthesia_if_indicated"]
          },
          "progression_criteria": ["ROM_improvements", "functional_gains"],
          "frequency": "daily_home_program_plus_2-3x_week_supervised"
        },
        "phase_3_thawing": {
          "duration": "12-24_months",
          "characteristics": ["gradual_ROM_return", "minimal_pain"],
          "goals": ["restore_full_ROM", "normalize_strength", "return_to_activities"],
          "interventions": {
            "exercises": ["EX_021", "EX_022", "EX_023", "functional_activities"],
            "progressive_strengthening": ["resistance_bands", "weights"],
            "sport_specific": ["if_applicable"]
          },
          "discharge_criteria": ["ROM_within_10%_normal", "functional_goals_met", "patient_satisfaction"]
        }
      },
      "outcome_measures": ["METRIC_003", "METRIC_015", "METRIC_020"],
      "evidence_references": ["Kelley_2013_Cochrane", "Page_2014_JOSPT"]
    },
