    "EX_226": {
      "name": "Pelvic Floor Muscle Training (Kegels)",
      "type": "pelvic_floor_strengthening",
      "body_region": "pelvic_floor",
      "difficulty_level": 2,
      "equipment_required": null,
      "equipment_optional": ["EQ_021", "biofeedback_unit"],
      "position": "various",
      "muscle_targets": {
        "primary": ["levator_ani", "coccygeus", "urethral_sphincter"],
        "secondary": ["deep_abdominals", "multifidus"]
      },
      "womens_health_focus": "incontinence_prolapse_prevention",
      "translations": {
        "en": {
          "name": "Pelvic Floor Muscle Training (Kegels)",
          "description": "Foundational exercise for pelvic floor strength and function",
          "instructions": [
            "Identify pelvic floor muscles by stopping urine midstream",
            "Contract pelvic floor muscles lifting up and in",
            "Hold for 3-5 seconds initially, progress to 10 seconds",
            "Relax completely between contractions",
            "Perform 10-15 repetitions, 3 times daily"
          ],
          "cues": [
            "Lift up like stopping urine flow",
            "Don't hold your breath",
            "Complete relaxation between contractions"
          ],
          "common_errors": [
            "Bearing down instead of lifting up",
            "Holding breath during exercise",
            "Not fully relaxing between contractions"
          ]
        }
      }
    },
