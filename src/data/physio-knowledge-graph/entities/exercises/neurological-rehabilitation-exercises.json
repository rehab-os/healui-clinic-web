      },
      "translations": {
        "en": {
          "name": "Canalith Repositioning (Epley Maneuver)",
          "description": "Specific maneuver to treat posterior canal BPPV",
          "instructions": [
            "Position patient sitting with legs extended",
            "Turn head 45 degrees toward affected ear",
            "Quickly move to lying position with head extended",
            "Turn head 90 degrees to opposite side",
            "Continue sequence to sitting position"
          ],
          "cues": [
            "Move quickly between positions",
            "Hold each position until vertigo stops",
            "Follow post-treatment precautions"
          ],
          "common_errors": [
            "Moving too slowly between positions",
            "Not completing full sequence",
            "Not following post-treatment restrictions"
          ]
        }
